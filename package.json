{
  "name": "ddd-boilerplate",
  "version": "6.0.1",
  "description": "Boilerplate for Domain Driven Design (DDD) in Typescript, Javascript",
  "main": "dist/index.js",
  "scripts": {
    "example-basic": "ts-node -r tsconfig-paths/register src/example/basic/index",
    "example-event": "ts-node -r tsconfig-paths/register src/example/event-sourcing/index",
    "build": "rm -rf dist && npx tsc --project tsconfig.json && tsc-alias -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nqd881/ddd-boilerplate"
  },
  "author": "QuocDai",
  "license": "ISC",
  "keywords": [
    "ddd",
    "ddd-boilerplate",
    "ddd-kit",
    "ddd-base",
    "ddd-foundation",
    "ddd-ts",
    "ddd-js"
  ],
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "type-fest": "^4.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.198",
    "@types/uuid": "^9.0.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
